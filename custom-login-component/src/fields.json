{ 
    "label": "Login",
    "classes": {
      "container": "container",
      "title": "title",
      "input": "input",
      "button": "btn"
    },
    "fields": [
    {
      "label": "Email: ",
      "name": "email",
      "type":"Email",
      "placeholder": "Enter Email..."
    },

    {
      "label": "Password: ",
      "name": "password",
      "type": "Password",
      "placeholder": "Enter Password..."
    }
  ],

  "regex": [
    {
      "emailRegex":"/^(([^<>()\\[\\]\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\.,;:\\s@\"]+)*)|(\".+\"))@(([^<>()[\\]\\.,;:\\s@\"]+\\.)+[^<>()[\\]\\.,;:\\s@\"]{2,})$/i",
      "pwdRegex": "^[a-zA-Z0-9_@!#()]{8,}"
    }
  ]
}